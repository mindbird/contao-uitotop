{
  "name": "mindbird/contao-uitotop",
  "description": "Small adjustable Button to go to top of the page",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords":["contao", "jquery", "toTop", "Seitenanfang"],
  "type":"contao-module",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de",
      "homepage": "http://www.mindbird.de"
    }
  ],
  "require": {
    "contao/core-bundle": "~3.2 || ~4.0",
    "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/uitotop"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "UiToTop\\": "src/"
    }
  },
  "replace": {
    "contao-legacy/uitotop": "self.version"
  }
}